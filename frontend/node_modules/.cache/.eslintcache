 totalBalance = () => {\n        return totalIncome() - totalExpenses();\n    };\n\n    const transactionHistory = () => {\n        const history = [...incomes, ...expenses];\n        history.sort((a, b) => {\n            return new Date(b.createdAt) - new Date(a.createdAt);\n        });\n        return history.slice(0, 3);\n    };\n\n    // Handle user signup\n    const handleSignup = (userData) => {\n        setUser(userData); // Set the user data in context\n    };\n\n    // Handle user signout\n    const handleSignout = () => {\n        setUser(null); // Clear user data on signout\n    };\n\n    return (\n        <GlobalContext.Provider value={{\n            addIncome,\n            getIncomes,\n            incomes,\n            deleteIncome,\n            expenses,\n            totalIncome,\n            addExpense,\n            getExpenses,\n            deleteExpense,\n            totalExpenses,\n            totalBalance,\n            transactionHistory,\n            error,\n            setError,\n            user,  // Provide user data to the app\n            handleSignup,  // Provide function to handle signup\n            handleSignout  // Provide function to handle signout\n        }}>\n            {children}\n        </GlobalContext.Provider>\n    );\n};\n\nexport const useGlobalContext = () => {\n    return useContext(GlobalContext);\n};\n","C:\\Users\\csai4\\Videos\\EXPENSE-TRACKER\\frontend\\src\\styles\\GlobalStyle.js",[],[],"C:\\Users\\csai4\\Videos\\EXPENSE-TRACKER\\frontend\\src\\styles\\Layouts.js",[],[],"C:\\Users\\csai4\\Videos\\EXPENSE-TRACKER\\frontend\\src\\Components\\Orb\\Orb.js",[],[],"C:\\Users\\csai4\\Videos\\EXPENSE-TRACKER\\frontend\\src\\Components\\Income\\Income.js",["234","235"],[],"import React, { useEffect } from 'react'\nimport styled from 'styled-components'\nimport { useGlobalContext } from '../../context/globalContext';\nimport { InnerLayout } from '../../styles/Layouts';\nimport Form from '../Form/Form';\nimport IncomeItem from 